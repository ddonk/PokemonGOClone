<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.FragmentInfoMoves">

    <TextView
        android:id="@+id/tvMove"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/readexprobold"
        android:paddingHorizontal="10dp"
        android:text="Move"
        android:textColor="#8F9195"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="27dp"
        tools:layout_conversion_absoluteWidth="53dp" />

    <TextView
        android:id="@+id/tvMove1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/readexprobold"
        android:paddingHorizontal="10dp"
        android:text="Move1: "
        android:textColor="#8F9195"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/tvMove"
        app:layout_constraintTop_toBottomOf="@+id/tvMove"
        tools:layout_conversion_absoluteHeight="27dp"
        tools:layout_conversion_absoluteWidth="53dp" />

    <TextView
        android:id="@+id/tvMove2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/readexprobold"
        android:paddingHorizontal="10dp"
        android:text="Move2: "
        android:textColor="#8F9195"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/tvMove1"
        app:layout_constraintTop_toBottomOf="@+id/tvMove1"
        tools:layout_conversion_absoluteHeight="27dp"
        tools:layout_conversion_absoluteWidth="53dp" />

    <TextView
        android:id="@+id/tvMove3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/readexprobold"
        android:paddingHorizontal="10dp"
        android:text="Move3"
        android:textColor="#8F9195"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/tvMove2"
        app:layout_constraintTop_toBottomOf="@+id/tvMove2"
        tools:layout_conversion_absoluteHeight="27dp"
        tools:layout_conversion_absoluteWidth="53dp" />
</androidx.constraintlayout.widget.ConstraintLayout>